{
  "publisher": "sahilng",
  "name": "dbtify",
  "displayName": "dbtify",
  "description": "VSCode extension to replace SQL table references with dbt model refs",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/sahilng/dbtify.git"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dbtify.transformEntireFile",
        "title": "dbtify: Transform Entire File"
      },
      {
        "command": "dbtify.transformSelection",
        "title": "dbtify: Transform Selection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
